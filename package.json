{
  "name": "eleventy-high-performance-blog",
  "version": "5.0.2",
  "license": "MIT",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build": "rm -rf _site && npm run js-build && eleventy && cp _redirects _site/ && cp _site/sitemap.xml _site/sitemap_index.xml && cp _site/feed.xml _site/feed && cp _site/feed.xml _site/atom.xml && cp _site/feed.xml _site/rss.xml",
    "watch": "concurrently \"eleventy --serve\" \"npm run js-build-watch\"",
    "serve": "eleventy --serve",
    "js-build": "rollup -c rollup.config.js",
    "js-build-watch": "rollup -c rollup.config.js -w",
    "debug": "DEBUG=* eleventy && npm run test",
    "test": "mocha test/test*.js",
    "test-watch": "mocha test/test*.js --watch",
    "deploy": "s3deploy -source _site -region us-east-1 -bucket ochronus.online -distribution-id E3K70CDJADQ4T3 -public-access -force -v"
  },
  "pre-push": [
    "build"
  ],
  "pre-commit": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "file-type": "^16.2.0",
    "fs-extra": "^9.1.0",
    "jsdom": "^16.4.0",
    "luxon": "^1.25.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.1",
    "node-fetch": "^2.6.1",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "shorthash": "0.0.2"
  },
  "dependencies": {
    "@ampproject/toolbox-optimizer": "^2.7.5",
    "@saschazar/wasm-avif": "^1.0.0",
    "clean-css": "^4.2.3",
    "concurrently": "^5.3.0",
    "csp-hash-generator": "0.0.3",
    "csso": "^4.2.0",
    "datauri": "^3.0.0",
    "eleventy-plugin-nesting-toc": "^1.2.0",
    "expect.js": "^0.3.1",
    "hasha": "^5.2.2",
    "html-minifier": "^4.0.0",
    "image-size": "^0.9.3",
    "jimp": "^0.16.1",
    "lru-cache": "^6.0.0",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-imsize": "^2.0.1",
    "mocha": "^8.2.1",
    "phin": "^3.5.1",
    "purge-from-html": "^1.0.3",
    "purgecss": "^4.0.0",
    "querystring": "^0.2.0",
    "rollup": "^2.38.1",
    "sharp": "^0.27.1",
    "svg-social-icons": "^1.1.0",
    "twitter": "^1.7.1"
  }
}
