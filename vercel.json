{
  "trailingSlash": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "interest-cohort=()"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/img/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public,max-age=31536000,immutable"
        }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public,max-age=31536000,immutable"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public,max-age=31536000,immutable"
        }
      ]
    }
  ],
